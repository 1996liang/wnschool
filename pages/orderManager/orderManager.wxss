
.titleHead{
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom:1px solid ghostwhite;
}
.titleHead image{
  width: 100rpx;
  height: 100rpx;
}
.orderType{
  font-size: 28rpx;
  color: lightslategray;
  margin-left: 25rpx;
  padding-top: 20rpx;
  padding-bottom:20rpx;
  display: flex;
  justify-content: flex-start;
}
.xingshi{
  font-size: 30rpx;
   padding-top: 5rpx;
  padding-bottom:5rpx;
  margin-left: 25rpx;
    /*margin-right: 25rpx;*/
    letter-spacing: 5px;
    font-weight: bold;
  color: mediumvioletred;
}
.element {
  
  padding-top: 50rpx;
  width: 100%;
  /*border-bottom: 1px solid gainsboro;*/
  position: relative;
  background-color: ghostwhite;
}

.head {
  display: flex;
  width: 80%;
}

.avatar {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  background-color: #4999fc;
  margin-left: 30rpx;
  margin-top: -10rpx;
}

.avatar image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}

.nickName {
  margin-left: 35rpx;
  margin-top: -15rpx;
}

.nickName text {
  color: #345685;
  font-size: 28rpx;
  font-weight: bold;
}


.msg {
  margin: 0 auto;
  width: 60%;
}

.content {
  color: #919191;
  font-size: 26rpx;
  font-weight: bold;
  letter-spacing: 2rpx;
  margin-top: -35rpx;
  margin-left: 15rpx;
}

.date {
  position: absolute;
  right: 5%;
  top: 3%;
}

.date text {
  color: #919191;
  font-size: 25rpx;
}
.status{
  display: flex;
  justify-content: flex-end;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
  font-size: 32rpx;
  color: #FE6B23;
  font-weight: bold;
  margin-right:25rpx; 
}
.money {
  width: 100rpx;
  height: 60rpx;
  position: absolute;
  right: 0%;
  top: 10%;
  z-index: 0;
}

.money canvas {
  width: 100%;
  height: 60rpx;
  display: block;
  z-index: 0;
}

.imageStatus {
  position: absolute;
  right: 3%;
  top: 5%;
}

.imageStatus image {
  width: 100px;
  height: 77px;
}

